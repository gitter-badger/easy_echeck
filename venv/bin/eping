#!/Users/zhangpenghong/Documents/workspace/easy_check/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'echeck==1.0.2','console_scripts','eping'
__requires__ = 'echeck==1.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('echeck==1.0.2', 'console_scripts', 'eping')()
    )
